{
  "name": "reactjs",
  "version": "1.0.0",
  "description": "setup react",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot --port=9000",
    "build:prod": "webpack -p && cp src/index.html dist/index.html"
  },
  "keywords": [
    "react"
  ],
  "author": "Neha Sharma",
  "license": "MIT",
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "webpack-cli": "^3.0.8"
  },
  "devDependencies": {
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "webpack": "^4.16.0",
    "webpack-dev-server": "^3.1.4"
  }
}
